{
  "pageName": "EventbiddingList",
  "page": {
    "title": "赛事申办列表",
    "search": {
      "use": true,
      "autoSearch": false,
      "model": "formSearch",
      "field": [
        {
          "elem": "input",
          "placeholder": "请输入申办单位名称",
          "model": "title",
          "default": "''"
        },
        {
          "elem": "input",
          "placeholder": "请输入选择申请人昵称",
          "model": "user",
          "default": "''"
        },
        {
          "elem": "select",
          "placeholder": "请选择联系状态",
          "model": "status",
          "default": "''",
          "source": "aStatus"
        },
        {
          "elem": "input",
          "placeholder": "请输入申办人姓名",
          "model": "name",
          "default": "''"
        }
      ],
      "action": [
        {
          "title": "导出Excel",
          "click": "fExport()",
          "type": "primary"
        }
      ]
    },
    "table": {
      "userPagination": true,
      "pageInfo": "pageInfo",
      "loading": "listLoading",
      "model": "tableLists",
      "fields": [
        {
          "label": "申请时间",
          "type": "string",
          "prop": "created_time"
        },
        {
          "label": "申请人昵称",
          "type": "string",
          "prop": "nickname"
        },
        {
          "label": "申办赛事",
          "type": "string",
          "prop": "title"
        },
        {
          "label": "申办单位",
          "type": "string",
          "prop": "company"
        },
        {
          "label": "所在地区",
          "type": "string",
          "prop": "position"
        },
        {
          "label": "申办人",
          "type": "string",
          "prop": "name"
        },
        {
          "label": "联系电话",
          "type": "string",
          "prop": "mobile"
        },
        {
          "label": "联系状态",
          "type": "fun",
          "scope": "scope",
          "template": "fShowStatus(scope.row)"
        },
        {
          "label": "操作",
          "type": "action",
          "scope": "scope",
          "action": [
            {
              "title": "标记为已联系",
              "click": "fSetStatus(scope.row)"
            }
          ]
        }
      ]
    },
    "dialog": {
      "addTitle": "添加地域",
      "editTitle": "编辑地域",
      "model": "formData",
      "field": [
      ]
    }
  },
  "js": {
    "name": "matchesEventbiddingList"
  },
  "source": [
    {
      "key": "aStatus",
      "value": [
        {"label": "未联系", "value": "0"},
        {"label": "已联系", "value": "1"}
      ]
    }
  ]
}