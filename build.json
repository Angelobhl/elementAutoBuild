{
  "pageName": "ScheduleList",
  "langKey": "scheduleList",
  "langPareng": "eventmanage",
  "page": {
    "title": "{title}[{type}]-赛程管理",
    "search": {
      "use": true,
      "autoSearch": false,
      "model": "formSearch",
      "field": [
        {
          "elem": "date-picker",
          "placeholder": "",
          "type": "datetimerange",
          "model": "dateRange",
          "default": "[]"
        },
        {
          "elem": "input",
          "placeholder": "请输入选手/战队名称",
          "model": "name",
          "default": "''"
        }
      ],
      "action": [
        {
          "title": "添加赛程",
          "click": "fEditOne({})",
          "type": "primary",
          "action": "addSchedule"
        }
      ]
    },
    "table": {
      "userPagination": true,
      "pageInfo": "pageInfo",
      "loading": "listLoading",
      "model": "tableLists",
      "fields": [
        {
          "label": "比赛场次",
          "type": "string",
          "prop": "group_id"
        },
        {
          "label": "比赛轮次",
          "type": "string",
          "prop": "stage_name"
        },
        {
          "label": "时间",
          "type": "fun",
          "scope": "scope",
          "prop": "time",
          "template": "scope.row.start_date"
        },
        {
          "label": "主场",
          "type": "fun",
          "scope": "scope",
          "prop": "team1",
          "template": "scope.row.team1 ? scope.row.team1.name : '-'"
        },
        {
          "label": "客场",
          "type": "fun",
          "scope": "scope",
          "prop": "team2",
          "template": "scope.row.team2 ? scope.row.team2.name : '-'"
        },
        {
          "label": "对局数",
          "type": "string",
          "prop": "bo"
        },
        {
          "label": "比分",
          "type": "fun",
          "scope": "scope",
          "prop": "result",
          "template": "(scope.row.team1 ? (scope.row.team1.result || '-') : '-') + ':' + (scope.row.team2 ? (scope.row.team2.result || '-') : '-')"
        },
        {
          "label": "选手积分",
          "type": "fun",
          "scope": "scope",
          "prop": "scoreList",
          "template": ""
        },
        {
          "label": "操作",
          "type": "action",
          "scope": "scope",
          "lang": "common.tableAction",
          "action": [
            {
              "title": "编辑",
              "click": "fEditOne(scope.row)",
              "lang": "common.editBtn"
            },
            {
              "title": "对局信息",
              "click": "fScheduleBoList(scope.row)",
              "lang": "common.editBtn"
            },
            {
              "title": "删除",
              "click": "fDelOne(scope.row)",
              "lang": "common.delBtn"
            }
          ]
        }
      ]
    },
    "dialog": {
      "addTitle": "添加赛程",
      "editTitle": "编辑赛程",
      "model": "formData",
      "field": [
        {
          "label": "比赛轮次",
          "elem": "input",
          "placeholder": "",
          "model": "stage_name",
          "default": "''"
        },
        {
          "label": "比赛时间",
          "elem": "date-picker",
          "type": "datetime",
          "model": "start_time",
          "default": "''"
        },
        {
          "label": "对局数",
          "elem": "inputNumber",
          "placeholder": "",
          "model": "bo",
          "default": "3",
          "max": "",
          "min": "0"
        },
        {
          "label": "对阵双方",
          "elem": "input",
          "placeholder": "",
          "model": "against",
          "default": "''"
        },
        {
          "label": "比分",
          "elem": "input",
          "placeholder": "",
          "model": "result",
          "default": "''"
        },
        {
          "label": "直播推流地址",
          "elem": "input",
          "placeholder": "",
          "model": "stream_url",
          "default": "''"
        },
        {
          "label": "视频ID",
          "elem": "input",
          "placeholder": "",
          "model": "stream_id",
          "default": "''"
        }
      ]
    }
  },
  "js": {
    "name": "eventmanageScheduleList"
  },
  "source": [
    {
      "key": "aSignList",
      "value": []
    }
  ]
}