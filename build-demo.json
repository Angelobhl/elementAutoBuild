{
  "pageName": "UserLog",
  "page": {
    "title": "用户查询",
    "search": {
      "use": true,
      "autoSearch": false,
      "model": "formSearch",
      "field": [
        {
          "elem": "input",
          "placeholder": "输入昵称/UID查询",
          "model": "title",
          "default": "''",
          "source": ""
        },
        {
          "elem": "date-picker",
          "placeholder": "输入昵称/UID查询",
          "type": "datetimerange",
          "model": "dateRange",
          "default": "[]"
        },
        {
          "elem": "select",
          "placeholder": "选择推荐位",
          "model": "position_id",
          "default": "''",
          "source": "positionSources"
        }
      ],
      "action": [
        {
          "title": "添加",
          "click": "fEditOne({})",
          "type": "primary"
        }
      ]
    },
    "table": {
      "userPagination": true,
      "pageInfo": "pageInfo",
      "loading": "listLoading",
      "model": "tableLists",
      "fields": [
        {
          "label": "UID",
          "type": "string",
          "prop": "uid",
          "width": "100"
        },
        {
          "label": "状态",
          "type": "fun",
          "scope": "scope.row",
          "template": "fShowStatus(scope.row)"
        },
        {
          "label": "操作",
          "type": "action",
          "scope": "scope.row",
          "action": [
            {
              "title": "编辑",
              "click": "fEditOne(scope.row)"
            },
            {
              "title": "删除",
              "click": "fEditOne(scope.row)"
            }
          ]
        }
      ]
    },
    "dialog": {
      "addTitle": "添加",
      "editTitle": "编辑",
      "model": "formData",
      "field": [
        {
          "label": "标题",
          "elem": "input",
          "placeholder": "输入昵称/UID查询",
          "model": "title",
          "default": "''",
          "source": ""
        },
        {
          "label": "开始结束时间",
          "elem": "date-picker",
          "type": "datetimerange",
          "model": "dateRange",
          "default": "[]",
          "source": ""
        },
        {
          "label": "状态",
          "elem": "select",
          "placeholder": "选择推荐位",
          "model": "position_id",
          "default": "''",
          "source": "positionSources"
        },
        {
          "label": "状态",
          "elem": "checkbox",
          "placeholder": "",
          "model": "position_id",
          "default": "[]",
          "source": "positionSources"
        },
        {
          "label": "状态",
          "elem": "radio",
          "placeholder": "",
          "model": "position_id",
          "default": "1",
          "source": "positionSources"
        }
      ]
    }
  },
  "js": {
    "name": "recommendManageRecommendation"
  },
  "source": [
    {
      "key": "positionSources",
      "value": [
        {"label": "启用", "value": "1"},
        {"label": "禁用", "value": "0"}
      ]
    },
    {
      "key": "aStatus",
      "value": [
        {"label": "启用", "value": "1"},
        {"label": "禁用", "value": "0"}
      ]
    }
  ]
}